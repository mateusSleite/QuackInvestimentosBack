{
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@vercel/node" }],
  "routes": [
      { "src": "/", "dest": "/" },
      { "src": "/user/login", "dest": "/" },
      { "src": "/user/register", "dest": "/" },
      { "src": "/investiment/create", "dest": "/" },
      { "src": "/investiment/getall", "dest": "/" },
      { "src": "/investiment/getid/", "dest": "/" },
      { "src": "/investiment/update/", "dest": "/" },
      { "src": "/investiment/delete/", "dest": "/" },
      { "src": "/(.*)", "dest": "/index.js" }
  ]    
}
